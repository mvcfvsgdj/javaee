<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.7.0/jquery.min.js">

</script>
<script>


	//$(document).ready(콜백함수);
	$(document).ready(function () {
		alert("1");
		$("#btn1").click( function(){
			alert("btn1 click");
		});

	
	let btn2Tag = document.querySelector("#btn2");
	btn2Tag.addEventListener("click", function(){
		alert("btn2 click")
	});
		});


</script>


<body>


	<button id="btn1">1</button>
	<button id="btn2">2</button>



</body>

