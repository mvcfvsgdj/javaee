	Calculator c = new Calculator();
	@Test
	void testAdd() {
		//fail("Not yet implemented");
		assertEquals(5, c.add(3, 5));
	}

	@Test
	void testSub() {
		//fail("Not yet implemented");
		assertEquals(5, c.sub(3, 5));
	}

	@Test
	void testMul() {
		//fail("Not yet implemented");
		assertEquals(5, c.mul(3, 5));
	}

	@Test
	void testDiv() {
		//fail("Not yet implemented");
		assertEquals(5, c.div(15, 3));
	}

	
}
