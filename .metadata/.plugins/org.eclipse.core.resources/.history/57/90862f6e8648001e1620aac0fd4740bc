package prj0901;

//class type 정의 <=고객정보 담을수있는 자료형
//class type 	변수를 만들때는 반드시 new를통해 변수감 ㅏㄹ들어진다 .변수=객체

public class Customer {
	
	String id;
	String pw;
	String name;
	
	
	public Customer(String id, String pw, String name) {
		super();
		this.id = id;
		this.pw = pw;
		this.name = name;
	}
	public Customer(){
		// TODO Auto-generated constructor stub
		
	}
	public String getId() {
		return id;
	}

	public void setId(String id) {
		this.id = id;
	}

	public String getPw() {
		return pw;
	}

	public void setPw(String pw) {
		this.pw = pw;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	@Override
	public String toString() {
		return "Customer [id=" + id + ", pw=" + pw + ", name=" + name + "]";
	}
}
